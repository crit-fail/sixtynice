{
  "description": "Adds a flag to all mentions of the number 69 to let you know it's nice.",
  "manifest_version": 2,
  "name": "SixtyNice",
  "version": "1.0",
  "homepage_url": "https://github.com/xdesro/sixtynice",
  "icons": {
    "48": "icons/sixtynice-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
